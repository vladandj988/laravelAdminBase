    "keywords": [
        "bytenet",
		"admin",
        "base"
    ],
    "homepage": "https://github.com/vladandj988/laravelAdminBase",
    "license": "GNU v3",
    "authors": [
        {
            "name": "Nikola Žeravčić",
            "email": "zexbre1@gmail.com",
            "homepage": "http://www.nikolazeravcic.iz.rs",
